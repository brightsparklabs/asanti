{
  pDPContextActivation(1),
  startOfInterceptionWithPDPContextActive(2),
  pDPContextDeactivation(4),
  gPRSAttach(5),
  gPRSDetach(6),
  cellOrRAUpdate(10),
  sMS(11),
  ...,
  pDPContextModification(13)
}

